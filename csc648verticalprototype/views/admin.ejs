<!-- Admin Tab Buttons -->
<div class="admin-nav"> 
<ul class="nav nav-tabs" id="myTab" role="tablist">
  <li class="nav-item">
    <a class="nav-link" id="pending-tab" data-toggle="tab" href="#pending" role="tab" aria-controls="pending" aria-selected="false">Pending Posts</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" id="approve-tab" data-toggle="tab" href="#approve" role="tab" aria-controls="approve" aria-selected="false">Approve Posts</a>
  </li>
    <li class="nav-item">
    <a class="nav-link" id="remove-tab" data-toggle="tab" href="#remove" role="tab" aria-controls="remove" aria-selected="false">Remove Posts</a>
  </li>
</ul>
<!-- Admin Tab Contents -->
<div class="tab-content" id="myTabContent">
  <div class="tab-pane fade" id="pending" role="tabpanel" aria-labelledby="pending-tab">
   <table class="table table-sm">
     <thead>
        <tr>
          <th scope="col">Post ID</th>
          <th scope="col">Image</th>
          <th scope="col">Post Name</th>
          <th scope="col">Price</th>
          <th scope="col">Description</th>
          <th scope="col">Category</th>
          <th scope="col">Status</th>
          <th scope="col">User ID</th>
        </tr>
    </thead>
    <% searchResult.forEach((result) => { %>
  <tbody class="post">   
  <tr>
  
      <th scope="row"><%= result.post_id%> </th>
      <td><%= result.image%> </td>
      <td><%= result.post_name%> </td>
      <td><%= result.price%> </td>
      <td><%= result.desciption%> </td>
      <td><%= result.category%> </td>
      <td><%= result.status%> </td>
      <td><%= result.user_id%> </td>
     
    </tr>
  </tbody>
   <% });%>
</table>
              
  </div>
  <div class="tab-pane fade" id="approve" role="tabpanel" aria-labelledby="approve-tab">
  Table to approve posts.
  </div>
    <div class="tab-pane fade" id="remove" role="tabpanel" aria-labelledby="remove-tab">
